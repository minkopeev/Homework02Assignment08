import java.util.Scanner;

public class Homework01Task04 {

    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        System.out.println("Please enter the first number:");

        int A = sc.nextInt();

        System.out.println("Please enter second number, different from the first number:");

        int B = sc.nextInt();

        if (A > B) {

            System.out.println("The numbers in ascending order are: " + B + ", " + A);
        }

        else {

            System.out.println("The numbers in ascending order are: " + A + ", " + B);
        }
    }
}
