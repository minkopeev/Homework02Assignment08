import java.util.Scanner;

public class Homework01Task01 {

    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        System.out.println("Please input A:");

        double a = sc.nextDouble();

        System.out.println("Please input B:");

        double b = sc.nextDouble();

        System.out.println("Please input c:");

        double c = sc.nextDouble();

        if (a > b) {

            if ((a > c) && (c > b)) {

                System.out.println("The number " + c + " is between the numbers " + b + " and " + a);

            } else {

                System.out.println("The number " + c + " is not between the numbers " + b + " and " + a);

            }

        } else {

            if (a < b) {

                if ((b > c) && (c > a)) {

                    System.out.println("The number " + c + " is between the numbers " + a + " and " + b);

                } else {

                    System.out.println("The number " + C + " is not between the numbers " + a + " and " + b);

                }

            }
