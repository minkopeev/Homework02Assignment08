import java.util.Scanner;

public class Homework02Task07 {

    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        System.out.println("Please enter number:");

        int n = sc.nextInt();

        int num = 3;

        for (int i = 1; i <= n; i++) {

            System.out.print(num + ", ");

            num += 3;
        }

    }
}
